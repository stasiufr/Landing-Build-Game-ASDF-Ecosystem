{
  "name": "asdf-skills-backend",
  "version": "1.0.0",
  "description": "Backend API for ASDF Skills Marketplace - Creation over Extraction",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "asdf",
    "skills",
    "marketplace",
    "solana",
    "github-api"
  ],
  "author": "ASDF Team",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.2.0",
    "supertest": "^6.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
